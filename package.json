{
	"name": "codeceptjs-tests",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"test": "npx codeceptjs run --tests --plugins allure --verbose",
		"codeceptjs": "codeceptjs run --steps",
		"codeceptjs:allure": "npx codeceptjs run --plugins allure",
		"show:report": "allure serve output",
		"codeceptjs:headless": "HEADLESS=true codeceptjs run --steps",
		"codeceptjs:ui": "codecept-ui --app",
		"codeceptjs:demo": "codeceptjs run --steps -c node_modules/@codeceptjs/examples",
		"codeceptjs:demo:headless": "HEADLESS=true codeceptjs run --steps -c node_modules/@codeceptjs/examples",
		"codeceptjs:demo:ui": "codecept-ui --app  -c node_modules/@codeceptjs/examples"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"./**/*.{js,jsx,ts,tsx}": [
			"npx prettier --write",
			"eslint **/*.js --fix-dry-run"
		]
	},
	"devDependencies": {
		"@codeceptjs/configure": "^0.6.2",
		"@codeceptjs/examples": "^1.2.1",
		"@codeceptjs/ui": "^0.4.3",
		"@wdio/selenium-standalone-service": "^7.4.2",
		"codeceptjs": "^3.0.6",
		"eslint": "^7.24.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-config-prettier": "^8.2.0",
		"eslint-plugin-codeceptjs": "^1.3.0",
		"eslint-plugin-import": "^2.22.1",
		"husky": "^4.2.3",
		"lint-staged": "^10.5.4",
		"playwright": "^1.10.0",
		"prettier": "^2.2.1"
	},
	"dependencies": {
		"-": "^0.0.1",
		"D": "^1.0.0",
		"eslint-config-airbnb": "^18.2.1",
		"xlsx": "^0.16.9"
	}
}
